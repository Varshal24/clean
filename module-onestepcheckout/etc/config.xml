<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Store:etc/config.xsd">
    <default>
        <aw_osc>
            <general>
                <enabled>1</enabled>
                <title>One Step Checkout</title>
                <apply_discount_code>1</apply_discount_code>
                <order_note_enabled>1</order_note_enabled>
                <google_autocomplete_enabled>0</google_autocomplete_enabled>
                <mini_cart_expanded>0</mini_cart_expanded>
                <display_top_menu>0</display_top_menu>
                <enable_checkout_statistics>1</enable_checkout_statistics>
                <enable_gc_and_coupon_code_field_merge>0</enable_gc_and_coupon_code_field_merge>
                <show_place_order_button_default>0</show_place_order_button_default>
                <is_allowed_to_change_product_options>1</is_allowed_to_change_product_options>
                <is_allowed_to_create_account_after_checkout>1</is_allowed_to_create_account_after_checkout>
            </general>
            <newsletter>
                <enable>0</enable>
                <checked_by_default>0</checked_by_default>
            </newsletter>
            <delivery_date>
                <available_weekdays>1,2,3,4,5</available_weekdays>
                <min_order_delivery_period>0</min_order_delivery_period>
                <same_day_delivery_unavailable_after>empty</same_day_delivery_unavailable_after>
                <next_day_delivery_unavailable_after>empty</next_day_delivery_unavailable_after>
                <note_is_enabled>0</note_is_enabled>
                <note>Delivery dates may vary</note>
            </delivery_date>
            <trust_seals>
                <enabled>0</enabled>
                <label>100% Secure Payment</label>
            </trust_seals>
            <geo_ip>
                <enabled>0</enabled>
            </geo_ip>
            <addresses_settings>
                <billing_shipping_are_the_same>1</billing_shipping_are_the_same>
                <address_type_to_display_first>shipping</address_type_to_display_first>
            </addresses_settings>
            <customer_info_customization>
                <display_customer_info_section>0</display_customer_info_section>
                <display_filled_fields>0</display_filled_fields>
            </customer_info_customization>
        </aw_osc>
        <sales>
            <totals_sort>
                <discount>80</discount>
            </totals_sort>
        </sales>
        <dev>
            <js>
                <minify_exclude>
                    https://www.gstatic.com/charts/loader.js
                </minify_exclude>
            </js>
        </dev>
    </default>
</config>
