<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:ObjectManager/etc/config.xsd">
    <type name="Aheadworks\OneStepCheckout\Model\Layout\LayoutProcessorProvider">
        <arguments>
            <argument name="processors" xsi:type="array">
                <item name="defaultProcessor" xsi:type="string">Aheadworks\OneStepCheckout\Model\Layout\Processor\DefaultProcessor</item>
                <item name="addressFormAttributes" xsi:type="string">Aheadworks\OneStepCheckout\Model\Layout\Processor\AddressAttributes</item>
                <item name="customerFormAttributes" xsi:type="string">Aheadworks\OneStepCheckout\Model\Layout\Processor\CustomerAttributes</item>
                <item name="cartItemDetails" xsi:type="string">Aheadworks\OneStepCheckout\Model\Layout\Processor\CartItemDetails</item>
                <item name="totals" xsi:type="string">Aheadworks\OneStepCheckout\Model\Layout\Processor\Totals</item>
                <item name="paymentOptions" xsi:type="string">Aheadworks\OneStepCheckout\Model\Layout\Processor\PaymentOptions</item>
                <item name="giftWrapping" xsi:type="string">Aheadworks\OneStepCheckout\Model\Layout\Processor\GiftWrapping\LayoutProcessor</item>
                <item name="beforePaymentExtraOptions" xsi:type="string">Aheadworks\OneStepCheckout\Model\Layout\Processor\BeforePaymentExtraOptions</item>
                <item name="deliveryDate" xsi:type="string">Aheadworks\OneStepCheckout\Model\Layout\Processor\DeliveryDate</item>
                <item name="htmlContent" xsi:type="string">Aheadworks\OneStepCheckout\Model\Layout\Processor\HtmlContent</item>
                <item name="additionalServices" xsi:type="string">Aheadworks\OneStepCheckout\Model\Layout\Processor\AdditionalServices</item>
                <item name="directoryData" xsi:type="string">Magento\Checkout\Block\Checkout\DirectoryDataProcessor</item>
                <item name="amazonServices" xsi:type="string">Aheadworks\OneStepCheckout\Model\Layout\Processor\AmazonServices</item>
                <item name="amazonPayServices" xsi:type="string">Aheadworks\OneStepCheckout\Model\Layout\Processor\AmazonPay</item>
                <item name="captcha_common" xsi:type="string">Aheadworks\OneStepCheckout\Model\Layout\Processor\Captcha\Common</item>
                <item name="captcha_mspReCaptcha" xsi:type="string">Aheadworks\OneStepCheckout\Model\Layout\Processor\Captcha\MspReCaptcha</item>
                <item name="captcha_magentoReCaptcha" xsi:type="string">Aheadworks\OneStepCheckout\Model\Layout\Processor\Captcha\MagentoReCaptcha</item>
                <item name="captcha_paymentMethod_paypalReCaptcha" xsi:type="string">Aheadworks\OneStepCheckout\Model\Layout\Processor\Captcha\PaymentMethod\PaypalReCaptcha</item>
                <item name="giftMessage" xsi:type="string">Aheadworks\OneStepCheckout\Model\Layout\Processor\GiftMessage</item>
                <item name="mergedGiftCardAndDiscount" xsi:type="string">Aheadworks\OneStepCheckout\Model\Layout\Processor\MergedGiftCardAndDiscount</item>
                <item name="captcha_paymentMethod_reCaptchaBraintree" xsi:type="string">Aheadworks\OneStepCheckout\Model\Layout\Processor\Captcha\PaymentMethod\ReCaptchaBraintree</item>
                <item name="inStorePickup_storePickup" xsi:type="string">Aheadworks\OneStepCheckout\Model\Layout\Processor\Inventory\InStorePickup\FrontendLayoutProcessor</item>
                <item name="configProcessor" xsi:type="string">Aheadworks\OneStepCheckout\Model\Layout\Processor\ConfigProcessor</item>
                <item name="removeUnusedPaymentRenders" xsi:type="string">Aheadworks\OneStepCheckout\Model\Layout\Processor\RemoveUnusedPaymentRenders</item>
            </argument>
        </arguments>
    </type>

    <virtualType name="Aheadworks\OneStepCheckout\Model\Layout\Processor\Inventory\InStorePickup\FrontendLayoutProcessor\Virtual"
                 type="Aheadworks\OneStepCheckout\Model\BaseFactory">
        <arguments>
            <argument name="moduleName" xsi:type="string">
                Magento_InventoryInStorePickupFrontend
            </argument>
            <argument name="className" xsi:type="string">
                Magento\InventoryInStorePickupFrontend\Block\Checkout\LayoutProcessor
            </argument>
        </arguments>
    </virtualType>
    <virtualType name="Aheadworks\OneStepCheckout\Model\InventorySalesApi\StockResolverFactory"
                 type="Aheadworks\OneStepCheckout\Model\BaseFactory">
        <arguments>
            <argument name="moduleName" xsi:type="string">
                Magento_InventorySalesApi
            </argument>
            <argument name="className" xsi:type="string">
                Magento\InventorySalesApi\Api\StockResolverInterface
            </argument>
        </arguments>
    </virtualType>
    <virtualType name="Aheadworks\OneStepCheckout\Model\InventoryIndexer\StockIndexTableNameResolverFactory"
                 type="Aheadworks\OneStepCheckout\Model\BaseFactory">
        <arguments>
            <argument name="moduleName" xsi:type="string">
                Magento_InventoryIndexer
            </argument>
            <argument name="className" xsi:type="string">
                Magento\InventoryIndexer\Model\StockIndexTableNameResolverInterface
            </argument>
        </arguments>
    </virtualType>
    <type name="Aheadworks\OneStepCheckout\Model\Layout\Processor\Inventory\InStorePickup\FrontendLayoutProcessor">
        <arguments>
            <argument name="inventoryInStorePickupFrontendLayoutProcessorFactory" xsi:type="object">
                Aheadworks\OneStepCheckout\Model\Layout\Processor\Inventory\InStorePickup\FrontendLayoutProcessor\Virtual
            </argument>
        </arguments>
    </type>
    <type name="Aheadworks\OneStepCheckout\Plugin\ConfigurableProduct\Model\ResourceModel\Attribute\OptionSelectBuilderPlugin">
        <arguments>
            <argument name="stockResolver" xsi:type="object">
                Aheadworks\OneStepCheckout\Model\InventorySalesApi\StockResolverFactory
            </argument>
            <argument name="stockIndexTableNameResolver" xsi:type="object">
                Aheadworks\OneStepCheckout\Model\InventoryIndexer\StockIndexTableNameResolverFactory
            </argument>
        </arguments>
    </type>
    <type name="Aheadworks\OneStepCheckout\Model\Layout\Processor\DeliveryDate">
        <arguments>
            <argument name="carrierCodeListToIgnore" xsi:type="array">
                <item name="inStorePickup" xsi:type="const">
                    Aheadworks\OneStepCheckout\Model\Shipping\Config::INVENTORY_IN_STORE_PICKUP_CARRIER_CODE
                </item>
            </argument>
        </arguments>
    </type>
    <type name="Aheadworks\OneStepCheckout\Model\ThirdPartyModule\Status\AmazonVersionPool">
        <arguments>
            <argument name="versionProcessors" xsi:type="array">
                <item name="default" xsi:type="string">Aheadworks\OneStepCheckout\Model\ThirdPartyModule\Status\Amazon\DefaultProcessor</item>
            </argument>
        </arguments>
    </type>
    <type name="Magento\Checkout\Model\CompositeConfigProvider">
        <arguments>
            <argument name="configProviders" xsi:type="array">
                <item name="one_step_checkout_config_provider" xsi:type="object">Aheadworks\OneStepCheckout\Model\ConfigProvider</item>
                <item name="one_step_checkout_config_shipping_address_provider" xsi:type="object">Aheadworks\OneStepCheckout\Model\ConfigProvider\DefaultAddress</item>
                <item name="one_step_checkout_payment_extended_config_provider" xsi:type="object">Aheadworks\OneStepCheckout\Model\ConfigProvider\PaymentMethodExtended</item>
                <item name="one_step_checkout_paypal_recaptcha_config_provider" xsi:type="object">Aheadworks\OneStepCheckout\Model\ThirdPartyModule\Paypal\ReCaptcha\Model\CheckoutConfigProvider</item>
                <item name="magento_gift_message_config_provider" xsi:type="object">Aheadworks\OneStepCheckout\Model\ThirdPartyModule\Magento\GiftMessage\CheckoutConfigProvider</item>
            </argument>
        </arguments>
    </type>
    <type name="Magento\Customer\Block\SectionConfig">
        <arguments>
            <argument name="clientSideSections" xsi:type="array">
                <item name="aw-osc-checkout-data" xsi:type="string">aw-osc-checkout-data</item>
            </argument>
        </arguments>
    </type>
    <type name="Magento\Customer\CustomerData\SectionPoolInterface">
        <arguments>
            <argument name="sectionSourceMap" xsi:type="array">
                <item name="aw-osc-gift-wrapping-info" xsi:type="string">Aheadworks\OneStepCheckout\CustomerData\GiftWrappingInfo</item>
            </argument>
        </arguments>
    </type>
    <type name="Aheadworks\OneStepCheckout\Model\Layout\Processor\RemoveUnusedPaymentRenders">
        <arguments>
            <argument name="paymentGroupsToCheck" xsi:type="array">
                <item name="0" xsi:type="string">payment_services</item>
            </argument>
        </arguments>
    </type>
    <type name="Magento\Checkout\Controller\Index\Index">
        <plugin name="aw_onestepcheckout_checkout_index_action_plugin" type="Aheadworks\OneStepCheckout\Plugin\Checkout\Controller\Index\Index"/>
    </type>
    <type name="Magento\Customer\Model\Delegation\Storage">
        <plugin name="aw_onestepcheckout_subscription_storage_plugin" type="Aheadworks\OneStepCheckout\Plugin\Customer\SubscriptionStoragePlugin"/>
    </type>
    <type name="Magento\ConfigurableProduct\Model\ResourceModel\Attribute\OptionSelectBuilderInterface">
        <plugin name="aw_onestepcheckout_in_stock_option_select_builder" type="Aheadworks\OneStepCheckout\Plugin\ConfigurableProduct\Model\ResourceModel\Attribute\OptionSelectBuilderPlugin" sortOrder="100"/>
    </type>

    <type name="Aheadworks\OneStepCheckout\Plugin\Customer\SubscriptionStoragePlugin">
        <arguments>
            <argument name="session" xsi:type="object">Magento\Customer\Model\Session\Proxy</argument>
        </arguments>
    </type>

    <type name="Aheadworks\OneStepCheckout\Model\Page\Initializer\ThirdPartyModuleList">
        <arguments>
            <argument name="modules" xsi:type="array">
                <item name="0" xsi:type="string">Ebizmarts_SagePaySuite</item>
                <item name="1" xsi:type="string">Adyen_Payment</item>
                <item name="2" xsi:type="string">Amazon_Payment</item>
                <item name="3" xsi:type="string">Cryozonic_StripePayments</item>
                <item name="4" xsi:type="string">Aheadworks_Nmi</item>
                <item name="5" xsi:type="string">StripeIntegration_Payments</item>
                <item name="6" xsi:type="string">AuthorizeNet_CreditCard</item>
                <item name="7" xsi:type="string">Amazon_Pay</item>
                <item name="8" xsi:type="string">Splitit_PaymentGateway</item>
            </argument>
        </arguments>
    </type>
    <type name="Aheadworks\OneStepCheckout\Model\Layout\Processor\BeforePaymentExtraOptions">
        <arguments>
            <argument name="componentsToMove" xsi:type="array">
                <item name="0" xsi:type="string">aw-reward-points</item>
                <item name="1" xsi:type="string">aw-store-credit</item>
                <item name="2" xsi:type="string">reward</item>
                <item name="3" xsi:type="string">storeCredit</item>
            </argument>
        </arguments>
    </type>
    <type name="Aheadworks\OneStepCheckout\Block\Checkout">
        <plugin name="klarnaKpOneStepCheckout" type="Aheadworks\OneStepCheckout\Plugin\Checkout\Block\OnepagePlugin"/>
    </type>
</config>
